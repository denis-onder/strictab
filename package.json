{
  "name": "strictab",
  "version": "1.0.0",
  "description": "Alternative new tab page",
  "main": "server/index.js",
  "scripts": {
    "css": "node-sass scss/style.scss -o web/",
    "css:watch": "npm run css && node-sass scss/style.scss -wo web/",
    "js": "npx babel ./js/main.js --out-file ./web/main.js",
    "js:watch": "npx babel ./js/main.js --watch --out-file ./web/main.js",
    "compile": "npm run css && npm run js",
    "start": "npm run css && npm run js && node ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/denis-onder/strictab.git"
  },
  "keywords": [],
  "author": "Denis Onder",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/denis-onder/strictab/issues"
  },
  "homepage": "https://github.com/denis-onder/strictab#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "concurrently": "^4.1.2",
    "eslint": "^6.2.2",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "mongoose": "^5.6.11",
    "opn": "^6.0.0"
  }
}
